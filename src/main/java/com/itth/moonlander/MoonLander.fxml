<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itth.moonlander.MoonLander">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                  <ToggleButton fx:id="buttonHuman" layoutX="10.0" layoutY="13.0" mnemonicParsing="false" onAction="#onHuman" text="Human" />
                  <ToggleButton fx:id="buttonTrain" mnemonicParsing="false" onAction="#onTrain" text="Train" />
                  <ToggleButton fx:id="buttonPlay" layoutX="10.0" layoutY="13.0" mnemonicParsing="false" onAction="#onPlay" text="Play" />
              </items>
            </ToolBar>
            <AnchorPane VBox.vgrow="SOMETIMES">
               <children>
                  <Pane fx:id="pane" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
